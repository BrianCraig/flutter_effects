#include <metal_stdlib>
#include <simd/simd.h>

using namespace metal;

struct TheBlock
{
    float theMember;
};

struct red_fragment_main_out
{
    float4 fragColor [[color(0)]];
};

fragment red_fragment_main_out red_fragment_main(constant TheBlock& _14 [[buffer(0)]])
{
    red_fragment_main_out out = {};
    out.fragColor = float4(1.0, cos(_14.theMember), 0.0, 1.0);
    return out;
}

